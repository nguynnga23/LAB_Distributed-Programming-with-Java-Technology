package Exercise02__Wait_Notisy_MechanismDemo;

public class MyQeue {
	int n;
	boolean valueSet = false;
	
	synchronized int get() {
		if(!valueSet) {
			try {
				wait();
			} catch (Exception e) {
				// TODO: handle exception
			}
		}
	}
	
	synchronized void put(int n) {
		this.n = n;
		System.out.println("Put: " + n);
	}
}
