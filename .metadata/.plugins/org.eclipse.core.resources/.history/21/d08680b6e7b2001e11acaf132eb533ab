package Exercise03;

public class Test {

	public static void main(String[] args) {
		Account a = new Account(0, 2309200320032l);

		Runnable r1 = new Runnable() {
			@Override
			public void run() {
				for(int i = 0; i < 10; i++)
					a.Deposit(100.0);
			}
		};

		Runnable r2 = new Runnable() {
			@Override
			public void run() {
				for(int i = 0; i < 5; i++)
					a.Withdraw(100.0);
			}
		};

		Thread t1 = new Thread(r1);
		Thread t2 = new Thread(r2);
		t1.start();
		t2.start();

		System.err.println("End");

	}

}
