package Exercise01;

import java.util.concurrent.Callable;

//1. 3. The third approach is implemented Callable interface
public class YourTask3 implements Callable<Long> {

	private String taskName;
//	private int counter;
	
	
	public YourTask3(String taskName) {
		this.taskName = taskName;
//		this.counter = counter;
	}

	
	@Override
	public Long call() throws Exception {
		// TODO Auto-generated method stub
		return null;
	}
	
	public static void main(String[] args) {
		Runnable r1 = new YourTask3("Print Task", 20);
		Runnable r2 = new YourTask3("Distribute Task", 23);
		Thread t1 = new Thread(r1);
		Thread t2 = new Thread(r2);
		t1.start();
		t2.start();
	}

	
}


